{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "InstrumentationKey": "your-instrumentation-key", // Замените на ваш ключ
    "EnableAdaptiveSampling": true, // Включение адаптивного сэмплинга
    "EnableDependencyTrackingTelemetryModule": true, // Включение отслеживания зависимостей
    "EnableRequestTrackingTelemetryModule": true // Включение отслеживания запросов
  }
}
